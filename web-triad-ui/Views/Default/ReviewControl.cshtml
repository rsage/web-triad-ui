@{
    ViewBag.Title = "ReviewControl";
}
<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Content/uiControls/Scripts/webTriadService.js"></script>
<script src="~/Content/uiControls/Scripts/reviewControl.js"></script>
<link rel="stylesheet" href="~/Content/uiControls/Css/tcStyle.css">

<div id="reviewControl" style="width: 80%; margin-top: 25px; margin-left: auto; margin-right: auto">
    
</div>

<script>
    $(document).ready(function() {

        $.ajaxSetup({
            beforeSend: function(xhr) {
                xhr.setRequestHeader("Authorization", "QUIC");
            }
        });

        var reviewControl = $("#reviewControl").reviewerSubmittedFiles({
            reviewData: [
                { Name: "ExamId", Value: "" },
                { Name: "FileGroup", Value: "" },
                { Name: "QualificationCode", Value: "" },
                { Name: "Status", Value: "Submitted" },
                { Name: "TimepointId", Value: "" }
            ]
        });

        //var timerId = setInterval(function() {
        //    reviewControl.reviewerSubmittedFiles("update");
        //}, 3000);
    });
</script>







